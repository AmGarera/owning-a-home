<div>

    <div class="summary content-l content-l__main">
        <div class="content-l_col content-l_col-2-3">
            Your summary
        </div>
        <div class="content-l_col content-l_col-1-3">
            Print
            Make changes
        </div>
    </div>
    
    <div class="summary summary-chart"></div>
    
    <div class="summary content-l content-l__main">
        <div class="content-l_col content-l_col-1-2">
            <section class="worksheet worksheet-summary">
              <h2><span class="cf-icon cf-icon-approved-round"></span> Goals</h2>
              {{> summarySection this.goals.[0]}}
              {{> summarySection this.goals.[1]}}
              {{> summarySection this.goals.[2]}}
            </section>
        </div>
        <div class="content-l_col content-l_col-1-2">
            <section class="worksheet worksheet-summary">
              <h2><span class="cf-icon cf-icon-complaint-round"></span> Red Flags</h2>
              {{> summarySection this.flags.[0]}}
              {{> summarySection this.flags.[1]}}
            </section>
            <section class="worksheet worksheet-summary">
              <h2><span class="cf-icon cf-icon-error-round"></span> Risks</h2>
              {{> summarySection this.risks.[2]}}
              {{> summarySection this.risks.[1]}}
            </section>
        </div>
    </div>
    
</div>